{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Compile C++ Project",
            "type": "shell",
            "command": "g++",
            "args": [
                "-Wall",
                "-Wextra",
                "-fdiagnostics-color=always",
                "-fopenmp",
                "-g3",
                "-std=c++11",
                "\"${workspaceFolder}/src/Sudoku Solver.cpp\"",
                "\"${workspaceFolder}/src/Game.cpp\"",
                "\"${workspaceFolder}/src/PossibleGrid.cpp\"",
                "\"${workspaceFolder}/src/Grid.cpp\"",
                "\"${workspaceFolder}/src/Pos.cpp\"",
                "-o",
                "\"${workspaceFolder}/src/output/Sudoku Solver\""
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            },
            "problemMatcher": "$gcc"
        }
    ]
}
